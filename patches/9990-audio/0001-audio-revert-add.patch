+static ULONG WINAPI AudioClient_AddRef(IAudioClient3 *iface)
+{
+    ACImpl *This = impl_from_IAudioClient3(iface);
+    ULONG ref;
+    ref = InterlockedIncrement(&This->ref);
+    TRACE("(%p) Refcount now %lu\n", This, ref);
