+    ok(hr == S_OK, "SetChannelVolume failed: %08lx\n", hr);
+
+    hr = IAudioStreamVolume_GetChannelVolume(asv, 0, &vol);
+    ok(hr == S_OK, "GetChannelVolume failed: %08lx\n", hr);
+    ok(fabsf(vol - 0.2f) < 0.05f, "Channel volume wasn't 0.2: %f\n", vol);
