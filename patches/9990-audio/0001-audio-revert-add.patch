+        *ppv = &This->IAudioStreamVolume_iface;
+    }else if(IsEqualIID(riid, &IID_IAudioSessionControl)){
+        if(!This->session_wrapper){
+            This->session_wrapper = AudioSessionWrapper_Create(This);
+            if(!This->session_wrapper){
+                LeaveCriticalSection(&g_sessions_lock);
