+    if(hr == E_ACCESSDENIED)
+        hr = IMMDevice_OpenPropertyStore(dev, STGM_READ, &store);
+    ok(hr == S_OK, "Opening valid store returned %08lx\n", hr);
+    if (store)
+    {
