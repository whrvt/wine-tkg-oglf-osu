+static UINT midi_in_start(WORD dev_id)
+{
+    TRACE("%d\n", dev_id);
+
+    if (dev_id >= num_srcs)
+    {
+        WARN("bad device ID : %d\n", dev_id);
+        return MMSYSERR_BADDEVICEID;
+    }
+    srcs[dev_id].state = 1;
+    srcs[dev_id].startTime = get_time_ms();
+    return MMSYSERR_NOERROR;
+}
