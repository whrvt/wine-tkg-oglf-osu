+        }else
+            ISimpleAudioVolume_AddRef(&This->session_wrapper->ISimpleAudioVolume_iface);
+
+        *ppv = &This->session_wrapper->ISimpleAudioVolume_iface;
+    }
+
+    if(*ppv){
+        LeaveCriticalSection(&g_sessions_lock);
+        return S_OK;
+    }
+
+    LeaveCriticalSection(&g_sessions_lock);
+
+    FIXME("stub %s\n", debugstr_guid(riid));
