+    if (IsEqualGUID(guid, &pulse_render_guid) && IsEqualPropertyKey(*prop, PKEY_AudioEndpoint_PhysicalSpeakers)) {
+        out->vt = VT_UI4;
+        out->ulVal = g_phys_speakers_mask;
+
+        return out->ulVal ? S_OK : E_FAIL;
+    }
+
+    return E_NOTIMPL;
+}
